<test_definition>
  <suite name="embedding-asyncapi-android-tests" category="Android embedding APIs">
    <set name="EmbeddingApiAsyncTest" type="androidunit" location="device">
      <testcase component="Crosswalk APIs/Embedding API" execution_type="auto" id="v7.XWalkViewTestAsync" purpose="Check if the methods of XWalkView interface can be executed correctly." subcase="10">
        <description>
          <test_script_entry>org.xwalk.embedding.asynctest.v7.XWalkViewTestAsync</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Embedding API" execution_type="auto" id="v7.XWalkSettingTestAsync" purpose="Check if the methods of XWalkSetting interface can be executed correctly." subcase="14">
        <description>
          <test_script_entry>org.xwalk.embedding.asynctest.v7.XWalkSettingTestAsync</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Embedding API" execution_type="auto" id="v7.HitTestTestAsync" purpose="Check if the getHitResult get correct hit type when long click on the web page."  subcase="22">
        <description>
          <test_script_entry>org.xwalk.embedding.asynctest.v7.HitTestTestAsync</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Embedding API" execution_type="auto" id="v7.OnCreateWindowRequestedTestAsync" purpose="Check if the XWalkView supports multiple windows."  subcase="2">
        <description>
          <test_script_entry>org.xwalk.embedding.asynctest.v7.OnCreateWindowRequestedTestAsync</test_script_entry>
        </description>
      </testcase>
    </set>
