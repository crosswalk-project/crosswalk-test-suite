        <description>
          <test_script_entry>/opt/apptools-android-tests/apptools/AppTools_Download.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="manual" id="Update_Download" purpose="Android - Validate if App Tool can download the crosswalk when update">
        <description>
          <test_script_entry>/opt/apptools-android-tests/apptools/Update_Download.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="manual" id="AppTools_Help" purpose="Android - Validate if all usages work well as description">
        <description>
          <test_script_entry>/opt/apptools-android-tests/apptools/AppTools_Help.html</test_script_entry>
        </description>
