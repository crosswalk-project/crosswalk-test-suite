          <test_script_entry>/opt/apptools-android-tests/apptools/build.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_create_remote" purpose="Android - Validate if Crosswalk tools can be downloaded when network well">
        <description>
          <test_script_entry>/opt/apptools-android-tests/apptools/create_remote.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_create_channel" purpose="Android - Validate if project can be created with difference Crosswalk channel" subcase="3">
        <description>
          <test_script_entry>/opt/apptools-android-tests/apptools/create_channel.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_create_version" purpose="Android - Validate if project can be created by Crosswalk version">
        <description>
          <test_script_entry>/opt/apptools-android-tests/apptools/create_version.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_update" purpose="Android - Validate if App Tool can work well with update option" subcase="10">
