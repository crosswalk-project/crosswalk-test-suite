          <test_script_entry>/opt/apptools-ios-tests/apptools/manifest_name.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_manifest_app_version" purpose="iOS - Validate if apk name contains app version" subcase="5">
        <description>
          <test_script_entry>/opt/apptools-ios-tests/apptools/manifest_app_version.py</test_script_entry>
        </description>
