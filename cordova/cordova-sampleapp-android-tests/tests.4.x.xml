    <set name="SampleApp_manual" type="js">
      <testcase component="Cordova Sample Apps/Spacedodge" execution_type="manual" id="Crosswalk_Cordova_SampleApp_Spacedodge_AppFunction" purpose="Validate if sample app Spacedodge function could work well">
        <description>
          <pre_condition>
            1. Make sure the Spacedodge webapp is launched.
          </pre_condition>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Crosswalk_Cordova_SampleApp_Spacedodge_AppFunction.html </test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Spacedodge" execution_type="manual" id="Crosswalk_Cordova_SampleApp_Spacedodge_Icon" purpose="Validate if sample app Spacedodge icon display normally">
        <description>
          <pre_condition>
            1. Make sure Spacedodge webapp is installed;
            2. Make sure Spacedodge webapp is available.
          </pre_condition>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Crosswalk_Cordova_SampleApp_Spacedodge_Icon.html </test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Spacedodge" execution_type="manual" id="Crosswalk_Cordova_SampleApp_Spacedodge_Switch" purpose="Validate if sample app Spacedodge could switch back successfully">
