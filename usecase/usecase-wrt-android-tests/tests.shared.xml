<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite name="usecase-wrt-android-tests">
    <set background="brand-primary" icon="glyphicon-asterisk" name="Web Runtime">
      <testcase component="Crosswalk Use Cases/WRT" execution_type="manual" id="CookieCheck" purpose="CookieCheck Test">
      </testcase>
      <testcase component="Crosswalk Use Cases/WRT" execution_type="manual" id="CopyPasteNative" purpose="CopyPasteNative Test">
      </testcase>
      <testcase component="Crosswalk Use Cases/WRT" execution_type="manual" id="OpenFile" purpose="OpenFile Test">
      </testcase>
      <testcase component="Crosswalk Use Cases/WRT" execution_type="auto" id="PermissionApiContacts" purpose="PermissionApiContacts Test">
      </testcase>
      <testcase component="Crosswalk Use Cases/WRT" execution_type="auto" id="SecurityUrlCertificate" purpose="SecurityUrlCertificate Test">
      </testcase>
      <testcase component="Crosswalk Use Cases/WRT" execution_type="manual" id="SSLCertificate" purpose="SSLCertificate Test">
      </testcase>
      <testcase component="Crosswalk Use Cases/WRT" execution_type="manual" id="WebAppHWKey" purpose="WebAppHWKey Test">
      </testcase>
      <testcase component="Crosswalk Use Cases/WRT" execution_type="auto" id="WebAppXwalkHost" purpose="WebAppXwalkHost Test">
      </testcase>
      <testcase component="Crosswalk Use Cases/WRT" execution_type="manual" id="CityPicker" purpose="City Picker Test">
      </testcase>
      <testcase component="Crosswalk Use Cases/WRT" execution_type="manual" id="WebMediaPlayer" purpose="Web Media Player Rotation Test">
      </testcase>
    </set>
    <set background="bs-purple3" icon="glyphicon-fire" name="Scheme">
      <testcase component="Crosswalk Use Cases/WRT" execution_type="manual" id="SchemesCheck" purpose="SchemesCheck">
      </testcase>
      <testcase component="Crosswalk Use Cases/WRT" execution_type="manual" id="CustomURLScheme" purpose="CustomURLScheme">
      </testcase>
    </set>
    <set name="SharedMode" background="brand-danger" icon="glyphicon-th-large">
      <testcase component="Crosswalk Use Cases/WRT" execution_type="manual" id="LocalDownload" purpose="LocalDownload Test">
      </testcase>
      <testcase component="Crosswalk Use Cases/WRT" execution_type="manual" id="WebAppsCoexist" purpose="WebAppsCoexist Test" subcase="2">
      </testcase>
    </set>
    <set name="Playstore Upgrade" background="brand-danger" icon="glyphicon-th-large">
      <testcase component="Crosswalk Use Cases/WRT" execution_type="manual" id="UpgradeLib" purpose="UpgradeLib Test" subcase="5">
      </testcase>
    </set>
  </suite>
</test_definition>
