{
  "pkg-blacklist": [
    "pack.py",
    "tests.*.xml",
    "icon.png",
    "manifest.json",
    "suite.json",
    "inst.*"
  ],
  "pkg-list": {
    "apk,cordova": {
      "blacklist": [
        "*",
        ".git"
      ],
      "copylist": {
        "inst.apk.py": "inst.py",
        "tests.android.xml": "tests.xml",
        "tests.full.xml": "tests.full.xml"
      },
      "subapp-list": {
        "": {
          "apk-type": "HOSTEDAPP",
          "apk-url-opt": "https://crosswalk-project.org/",
          "app-name": "remotedebugging-no",
          "install-path": "."
        },
        ".": {
          "app-name": "usecase-litewrt-android-tests",
          "blacklist": [
            "*",
            ".git"
          ],
          "copylist": {
            "PACK-TOOL-ROOT/bootstrap-fw": ".",
            "icon.png": "icon.png",
            "manifest.json": "manifest.json",
            "tests": "samples",
            "steps": "steps",
            "tests.android.xml": "tests.xml"
          },
          "install-path": "."
        },
        "tests/VideoWebSite/res/": {
          "apk-icon-opt": "",
          "apk-type": "MANIFEST",
          "app-name": "videowebsitetest",
          "install-path": "."
        }
      }
    }
  },
  "pkg-name": "usecase-litewrt-android-tests"
}
