{
    "pkg-blacklist": [
        "config.xml", 
        "pack.py", 
        "testcase.xsl", 
        "testresult.xsl", 
        "tests.css", 
        "icon.png", 
        "manifest.json", 
        "suite.json", 
        "*-py", 
        "inst.*"
    ], 
    "pkg-list": {
        "apk,cordova": {
            "blacklist": [], 
            "copylist": {
                "inst.apk.py": "inst.py", 
                "tests.android.xml": "tests.xml", 
                "tests.full.xml": "tests.full.xml"
            }, 
            "subapp-list": {
                ".": {
                    "app-name": "wrt-usecase-android-tests", 
                    "copylist": {
                        "tests.android.xml": "tests.xml" 
                    }, 
                    "blacklist": [
                        "manifest.json"
                    ], 
                    "install-path": "."
                }, 
                "tests/CopyPasteNative/res": {
                    "app-name": "copypastenative", 
                    "apk-icon-opt": "",
                    "install-path": "."
                }, 
                "tests/ExtensionPermission/res": {
                    "apk-ext-opt": "tests/ExtensionPermission/res/contactextension", 
                    "app-name": "extensionpermission", 
                    "install-path": "."
                }, 
                "tests/OpenFile/res": {
                    "app-name": "openfile", 
                    "apk-icon-opt": "",
                    "install-path": "."
                }, 
                "tests/PermissionApiContacts/res": {
                    "app-name": "permissionapicontacts", 
                    "apk-icon-opt": "",
                    "install-path": "."
                }, 
                "tests/RemoteDebugging/res": {
                    "apk-cmd-opt": "--enable-remote-debugging", 
                    "apk-icon-opt": "",
                    "app-name": "remotedebugging", 
                    "install-path": "."
                }, 
                "tests/SchemesCheck/res": {
                    "app-name": "schemescheck", 
                    "apk-icon-opt": "",
                    "install-path": "."
                }, 
                "tests/SharemodeLibrary/res": {
                    "apk-mode-opt": "shared", 
                    "apk-icon-opt": "",
                    "app-name": "sharemodelibrary", 
                    "install-path": "."
                }, 
                "tests/SplashscreenLandscape/res": {
                    "app-name": "splashscreenlandscape", 
                    "apk-icon-opt": "",
                    "install-path": "."
                }, 
                "tests/SplashscreenPng/res": {
                    "app-name": "splashscreenpng", 
                    "apk-icon-opt": "",
                    "install-path": "."
                }, 
                "tests/WebAppGesture/res": {
                    "app-name": "webappgesture", 
                    "apk-icon-opt": "",
                    "install-path": "."
                }, 
                "tests/WebAppHWKey/res": {
                    "app-name": "webapphwkey", 
                    "apk-icon-opt": "",
                    "install-path": "."
                }, 
                "tests/WebAppManifest/res": {
                    "apk-type": "MANIFEST", 
                    "app-name": "webappmanifest", 
                    "install-path": "."
                }, 
                "tests/WebAppXwalkHost/res": {
                    "app-name": "webappxwalkhost", 
                    "apk-icon-opt": "",
                    "install-path": "."
                }
            }
        }, 
        "apk-aio": {
            "blacklist": []
        }, 
        "wgt": {
            "blacklist": [], 
            "copylist": {
                "inst.wgt.py": "inst.py", 
                "tests.full.xml": "tests.full.xml", 
                "tests.xml": "tests.xml"
            }, 
            "pkg-app": {
                "blacklist": [], 
                "sign-flag": "true"
            }
        }, 
        "xpk": {
            "blacklist": [], 
            "copylist": {
                "inst.xpk.py": "inst.py", 
                "tests.full.xml": "tests.full.xml", 
                "tests.xml": "tests.xml"
            }, 
            "pkg-app": {
                "blacklist": []
            }
        }
    }, 
    "pkg-name": "wrt-usecase-android-tests"
}
