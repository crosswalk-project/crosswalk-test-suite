{
  "pkg-blacklist": [
    "pack.py",
    "tests.*.xml",
    "icon.png",
    "manifest.json",
    "suite.json",
    "inst.*"
  ],
  "pkg-list": {
    "apk,cordova": {
      "blacklist": [
        "*",
        ".git"
      ],
      "copylist": {
        "PACK-TOOL-ROOT/atip/tests/environment.py": "testscripts/environment.py",
        "PACK-TOOL-ROOT/resources/bdd/bddrunner": "bddrunner",
        "PACK-TOOL-ROOT/resources/bdd/data.conf": "data.conf",
        "data": "data",
        "inst.apk.py": "inst.py",
        "samples/CSP/res/resources": "csp/resources",
        "tests.android.xml": "tests.xml",
        "tests.auto.xml": "tests.auto.xml",
        "tests.full.xml": "tests.full.xml",
        "testscripts": "testscripts"
      },
      "subapp-list": {
        ".": {
          "apk-common-opts": "--enable-remote-debugging",
          "app-name": "usecase-webapi-xwalk-tests",
          "blacklist": [
            "*",
            ".git"
          ],
          "copylist": {
            "PACK-TOOL-ROOT/bootstrap-fw": ".",
            "icon.png": "icon.png",
            "manifest.json": "manifest.json",
            "res": "res",
            "samples": "samples",
            "steps": "steps",
            "tests.android.xml": "tests.xml"
          },
          "install-path": "."
        },
        "samples/Advertising/res": {
          "apk-ext-opt": "samples/Advertising/res/ad",
          "app-name": "extensions_ad",
          "app_dir": "samples/Advertising/res",
          "copylist": {
            "PACK-TOOL-ROOT/bootstrap-fw/js/jquery-2.1.3.min.js": "js/jquery.js"
          },
          "install-path": "."
        },
        "samples/InAppPayment/iap": {
          "apk-common-opts": "--permissions=iap",
          "apk-ext-opt": "samples/InAppPayment/iap",
          "apk-icon-opt": ""
        }
      }
    },
    "msi": {
      "blacklist": [
        "*",
        ".git"
      ],
      "copylist": {
        "PACK-TOOL-ROOT/atip/tests/environment.py": "testscripts/environment.py",
        "PACK-TOOL-ROOT/resources/bdd/bddrunner": "bddrunner",
        "PACK-TOOL-ROOT/resources/bdd/data.conf": "data.conf",
        "data": "data",
        "samples/CSP/res/resources": "csp/resources",
        "tests.windows.xml": "tests.xml",
        "tests.auto.xml": "tests.auto.xml",
        "tests.full.xml": "tests.full.xml",
        "testscripts": "testscripts"
      },
      "subapp-list": {
        ".": {
          "apk-common-opts": "--enable-remote-debugging",
          "app-name": "usecase-webapi-xwalk-tests",
          "blacklist": [
            "*",
            ".git"
          ],
          "copylist": {
            "PACK-TOOL-ROOT/bootstrap-fw": ".",
            "icon.ico": "icon.ico",
            "manifest.windows.json": "manifest.json",
            "res": "res",
            "samples": "samples",
            "steps": "steps",
            "tests.windows.xml": "tests.xml"
          },
          "install-path": "."
        }
      }
    }
  },
  "pkg-name": "usecase-webapi-xwalk-tests"
}
