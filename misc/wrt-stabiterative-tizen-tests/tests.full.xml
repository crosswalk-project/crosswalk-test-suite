<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite name="wrt-stabiterative-tizen-tests" category="Stability" launcher="xwalk">
    <set name="stabiterative" type="js">
      <testcase purpose="Validate launch and exit web apps repeatedly no error" type="Functional" status="approved" component="Stability" execution_type="manual" priority="P1" id="Crosswalk_Stabiterative_LaunchAndExit_Repeatedly">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed on device;
            2.Download package resources from otcqa to local;
            3.Push package resources to device,run "sdb push xxxx /".
          </pre_condition>
          <test_script_entry>/opt/wrt-stabiterative-tizen-tests/stabiterative/Crosswalk_Stabiterative_LaunchAndExit_Repeatedly.html </test_script_entry>
       </description>
      </testcase>
          <testcase purpose="Validate open and close window repeatedly no error" type="Functional" status="approved" component="Stability" execution_type="manual" priority="P1" id="Crosswalk_Stabiterative_OpenAndClose_Repeatedly">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed on device;
            2.Download package resources from otcqa to local;
            3.Push package resources to device,run "sdb push xxxx /".
          </pre_condition>
          <test_script_entry>/opt/wrt-stabiterative-tizen-tests/stabiterative/Crosswalk_Stabiterative_OpenAndClose_Repeatedly.html </test_script_entry>
          </description>
      </testcase>
          <testcase purpose="Validate switch native app and webapp repeatedly no error" type="Functional" status="designed" component="Stability" execution_type="manual" priority="P1" id="Crosswalk_Stabiterative_SwitchApp_Repeatedly">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed on device;
            2.Download package resources from otcqa to local;
            3.Push package resources to device,run "sdb push xxxx /".
          </pre_condition>
          <test_script_entry>/opt/wrt-stabiterative-tizen-tests/stabiterative/Crosswalk_Stabiterative_SwitchApp_Repeatedly.html </test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>
