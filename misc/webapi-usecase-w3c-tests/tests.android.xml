<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite name="webapi-usecase-w3c-tests">
    <set name="DOM, Forms and Styles">
      <testcase component="usecase" execution_type="manual" id="CSSStyle" purpose="CSS Style Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="FlexibleBox" purpose="Flexible Box Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="CSSAnimation" purpose="CSS Animation Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Transitions" purpose="Transitions Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="CSSStyleAttribute" purpose="CSS Style Attribute Test" subcase="4">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="AnimationTransform" purpose="Animation Transform Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="AnimationTiming" purpose="Animation Timing Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="MediaQueries" purpose="Media Queries Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Forms" purpose="Forms Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Input" purpose="Input Test" subcase="3">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Selectors" purpose="Selectors Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Jsenhance" purpose="Jsenhance Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="HTMLTemplate" purpose="HTML Template Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="ShadowDom" purpose="ShadowDom Test">
      </testcase>
    </set>
    <set name="Device">
      <testcase component="usecase" execution_type="manual" id="Touch" purpose="Touch Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Vibration" purpose="Vibration Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="ScreenOrientation" purpose="Screen Orientation Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="DeviceCapabilities" purpose="Device Capabilities Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Accelerometer" purpose="Accelerometer Test">
        <capability name="accelerometer"/>
      </testcase>
      <testcase component="usecase" execution_type="manual" id="BatteryStatus" purpose="Battery Status Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="BrowserState" purpose="Browser State Test">
      </testcase>
    </set>
    <set name="Graphics">
      <testcase component="usecase" execution_type="manual" id="SVGTransformation" purpose="SVG Transformation Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Canvas" purpose="Canvas Test">
      </testcase>
    </set>
    <set name="Media">
      <testcase component="usecase" execution_type="manual" id="AudioPlay" purpose="Audio Play Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="VideoPlayAndroid" purpose="Video Play Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="WebAudio" purpose="WebAudio Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Camera" purpose="Camera Test">
        <capability name="camera"/>
      </testcase>
      <testcase component="usecase" execution_type="manual" id="CameraViaUserMedia" purpose="Camera via UserMedia Test">
        <capability name="cameraFront"/>
      </testcase>
    </set>
    <set name="Communication">
      <testcase component="usecase" execution_type="manual" id="WebMessaging" purpose="Web Messaging Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="RawSockets" purpose="Raw Sockets Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="WebSpeech" purpose="Web Speech Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="WebRTC" purpose="WebRTC Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="WebSocket" purpose="WebSocket Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="SessionHistory" purpose="Session History Test">
      </testcase>
    </set>
    <set name="Storage">
      <testcase component="usecase" execution_type="manual" id="FileReader" purpose="FileReader Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="FileSystem" purpose="File System Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="IndexedDB" purpose="IndexedDB Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="WebDatabase" purpose="Web Database Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="WebStorage" purpose="Web Storage Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="ContactsManager" purpose="Contacts Manager Test">
      </testcase>
    </set>
    <set name="Security">
      <testcase component="usecase" execution_type="manual" id="SandboxAndroid" purpose="Sandbox Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="CSP" purpose="CSP Test" subcase="9">
      </testcase>
    </set>
    <set name="UI">
      <testcase component="usecase" execution_type="manual" id="Clipboard" purpose="Clipboard Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Notifications" purpose="Notifications Test">
      </testcase>
    </set>
    <set name="Performance and Optimization">
      <testcase component="usecase" execution_type="manual" id="PageVisibility" purpose="Page Visibility Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Workers" purpose="Workers Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="HrtimeAndroid" purpose="Hrtime Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="NavigationTiming" purpose="Navigation Timing Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="ResourceTiming" purpose="Resource Timing Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Usertiming" purpose="User Timing Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="PerformanceTimeline" purpose="Performance Timeline Test">
      </testcase>
    </set>
    <set name="Location">
      <testcase component="usecase" execution_type="manual" id="LocationGPS" purpose="Location GPS Test">
        <capability name="locationGps"/>
      </testcase>
    </set>
    <set name="Supplementary">
      <testcase component="usecase" execution_type="manual" id="WebGL" purpose="WebGL Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="TypedArray" purpose="Typed Array Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="FullScreen" purpose="Full Screen Test">
      </testcase>
    </set>
    <set name="Others">
      <testcase component="usecase" execution_type="manual" id="AppURI" purpose="App URI Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Presentation" purpose="Presentation Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="SIMD" purpose="SIMD Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="GamePad" purpose="GamePad Test">
        <capability name="accelerometer"/>
      </testcase>
    </set>
  </suite>
</test_definition>
