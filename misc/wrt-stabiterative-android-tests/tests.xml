<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="Stability" name="wrt-stabiterative-android-tests">
    <set name="stabiterative" type="js">
      <testcase purpose="Validate launch and exit web apps repeatedly no error" component="Stability" execution_type="manual" id="Crosswalk_Stabiterative_LaunchAndExit_Repeatedly">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed on device;
            2.Download package resources from otcqa to local;
            3.Push package resources to device,run "sdb push xxxx /".
          </pre_condition>
          <test_script_entry>/opt/wrt-stabiterative-android-tests/stabiterative/Crosswalk_Stabiterative_LaunchAndExit_Repeatedly.html </test_script_entry>
          </description>
          </testcase>
      <testcase purpose="Validate playing webaudio 1500 times" component="Stability" execution_type="manual" id="Crosswalk_Stabiterative_Playing_Repeatedly">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed on device;
          </pre_condition>
          <test_script_entry>/opt/wrt-stabiterative-android-tests/stabiterative/Crosswalk_Stabiterative_Playing_Repeatedly.html </test_script_entry>
          </description>
          </testcase>          
    </set>
  </suite>
</test_definition>
