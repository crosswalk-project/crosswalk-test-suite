<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl"  href="./testcase.xsl"?>
<test_definition>
  <suite name="tct-fullscreen-nonw3c-tests" launcher="xwalk" category="Supplementary APIs">
    <set name="FullScreen" type="js">
