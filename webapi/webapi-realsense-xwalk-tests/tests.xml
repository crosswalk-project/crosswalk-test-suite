          <test_script_entry>/opt/webapi-realsense-xwalk-tests/realsense/photoutils_depthresize-manual.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/RealSense" execution_type="manual" id="photoutils_enhancedepth" purpose="Test checks that enhancing the depth map image with high quality">
