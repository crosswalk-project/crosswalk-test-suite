          <test_script_entry>/opt/webapi-realsense-xwalk-tests/realsense/photo_queryimage-manual.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/RealSense" execution_type="auto" id="photo_querynumberofcameras" purpose="Test checks that queryNumberOfCameras() method gets the number of cameras" onload_delay="10">
