          <test_script_entry>/opt/webapi-realsense-xwalk-tests/realsense/photoutils_commonfov-manual.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/RealSense" execution_type="manual" id="photoutils_depthresize" purpose="Test checks that resizing the photo's depth map to to 640px width">
