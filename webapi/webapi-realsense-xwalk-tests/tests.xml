        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/RealSense" execution_type="auto" id="photo_resetcontainerimage" purpose="Test checks that resetContainerImage() copies the camera[0] unedited color image to be the container image of the photo instance" onload_delay="10">
