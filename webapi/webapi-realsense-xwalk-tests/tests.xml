        <description>
          <test_script_entry>/opt/webapi-realsense-xwalk-tests/realsense/photoutils_getdepthquality.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/RealSense" execution_type="manual" id="measurement_measuredistance" purpose="Test checks that get the distance from the start point to the end point">
