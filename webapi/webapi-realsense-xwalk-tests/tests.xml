      </testcase>
      <testcase component="Crosswalk APIs/Experimental/RealSense" execution_type="auto" id="photo_queryxdmrevision" purpose="Test checks that queryXDMRevision() gets the XDM file revision" onload_delay="10">
        <description>
          <test_script_entry>/opt/webapi-realsense-xwalk-tests/realsense/photo_queryxdmrevision.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/RealSense" execution_type="manual" id="photoutils_photorotate" purpose="Test checks that the photo is rotated 90 degree">
        <description>
          <test_script_entry>/opt/webapi-realsense-xwalk-tests/realsense/photoutils_photorotate-manual.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/RealSense" execution_type="manual" id="photoutils_colorresize" purpose="Test checks that resizing photo's color image to 800px width">
        <description>
          <test_script_entry>/opt/webapi-realsense-xwalk-tests/realsense/photoutils_colorresize-manual.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/RealSense" execution_type="manual" id="photoutils_commonfov" purpose="Test checks that commonFOV() returns the new photo instance">
