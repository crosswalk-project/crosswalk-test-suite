    "pack.py",
    "testcase.xsl",
    "testresult.xsl",
    "tests.css",
    "icon.png",
    "manifest.json",
    "suite.json",
    "inst.*"
  ],
  "pkg-list": {
    "apk,cordova": {
      "blacklist": [
        "*"
      ],
      "copylist": {
        "inst.apk.py": "inst.py",
        "tests.xml": "tests.xml"
      },
      "pkg-app": {
        "blacklist": [
