<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="Native Client" name="webapi-nacl-xwalk-tests">
    <set name="nacl" type="js">
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_Audio" purpose="Check if the Sine Wave synthesizer example demonstrates playing sound">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_Audio.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_Core" purpose="Check if the Core API example demonstrates the basic use of the Time and Callback functions">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_Core.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_File_IO" purpose="Check if the File IO example demonstrates saving, loading, and deleting files from the persistent file store">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_File_IO.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_Graphics2D" purpose="Check if the Graphics2D example demonstrates a simple flame effect">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_Graphics2D.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_Graphics3D" purpose="Check if the Graphics3D example demonstrates how to create a 3D cube that rotates">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_Graphics3D.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_Input_Event" purpose="Check if the example shows how to handle input events">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_Input_Event.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_MediaStream_Video" purpose="Check if the MediaStream Video example demonstrates how to capture video from a webcam">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_MediaStream_Video.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_Mouse_Cursor" purpose="Check if the example shows how to change the mouse cursor">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_Mouse_Cursor.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_Mouse_Lock" purpose="Check if the mouse movement is maintained even in tab and/or browser fullscreen">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_Mouse_Lock.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_Network_Monitor" purpose="Check if the Network Monitor example demonstrates how to listen for changes to the status of the network interfaces">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_Network_Monitor.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_Socket" purpose="Check if the socket example demonstrates how to use the TCP and UDP socket API">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_Socket.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_URL_Loader" purpose="Check if the URL Loader example demonstrates fetching an URL and then displaying its contents">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_URL_Loader.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_Var_Array_Buffer" purpose="Check if the VarArrayBuffer example demonstrates prompting the use for a file, then drawing a histogram representing the contents of the file to a 2D square">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_Var_Array_Buffer.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_Var_Dictionary" purpose="Check if the example shows how to use the pp::VarDictionary var type">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_Var_Dictionary.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_Websocket" purpose="Check if the Websocket example demonstrates how to use the Websocket API">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_Websocket.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Native Client" execution_type="manual" id="Nacl_api_MediaStream_Audio" purpose="Check if the MediaStream Audio example demonstrates how to capture audio data from an audio input device">
        <description>
          <test_script_entry>/opt/webapi-nacl-xwalk-tests/nacl/Nacl_api_MediaStream_Audio.html</test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>