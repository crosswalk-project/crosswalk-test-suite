{
  "pkg-blacklist": [
    "pack.py",
    "testcase.xsl",
    "testresult.xsl",
    "tests.css",
    "icon.png",
    "manifest.json",
    "suite.json",
    "*-py",
    "inst.*"
  ],
  "pkg-list": {
    "apk,cordova": {
      "blacklist": [],
      "copylist": {
        "PACK-TOOL-ROOT/atip/tests/environment.py": "html5-sandbox-app/testscripts/environment.py",
        "PACK-TOOL-ROOT/resources/bdd/bddrunner": "bddrunner",
        "PACK-TOOL-ROOT/resources/testharness": "resources",
        "PACK-TOOL-ROOT/resources/webrunner": "webrunner",
        "html5-sandbox-app/testscripts": "html5-sandbox-app/testscripts",
        "inst.apk.py": "inst.py",
        "tests.full.xml": "tests.full.xml",
        "tests.xml": "tests.xml"
      },
      "pkg-app": {
        "blacklist": [],
        "hosted-app": "true"
      },
      "subapp-list": {
        "html5-sandbox-app": {
          "apk-common-opts": "--enable-remote-debugging",
          "apk-type": "MANIFEST",
          "copylist": {
            "../sandbox/w3c/bdd": "w3c",
            "../sandbox/w3c/support": "w3c/support"
          }
        }
      }
    },
    "apk-aio, cordova-aio": {
      "blacklist": [],
      "copylist": {
        "PACK-TOOL-ROOT/atip/tests/environment.py": "HOST_RESOURCES/html5-sandbox-app/testscripts/environment.py",
        "PACK-TOOL-ROOT/resources/testharness": "resources",
        "PACK-TOOL-ROOT/resources/webrunner": "webrunner",
        "html5-sandbox-app/testscripts": "HOST_RESOURCES/html5-sandbox-app/testscripts"
      },
      "subapp-list": {
        "html5-sandbox-app": {
          "apk-common-opts": "--enable-remote-debugging",
          "apk-type": "MANIFEST",
          "copylist": {
            "../sandbox/w3c/bdd": "w3c",
            "../sandbox/w3c/support": "w3c/support"
          }
        }
      }
    },
    "msi": {
      "blacklist": [
        "*"
      ],
      "copylist": {
        "PACK-TOOL-ROOT/atip/tests/environment.py": "html5-sandbox-app/testscripts/environment.py",
        "PACK-TOOL-ROOT/resources/bdd/bddrunner": "bddrunner",
        "PACK-TOOL-ROOT/resources/testharness": "resources",
        "PACK-TOOL-ROOT/resources/webrunner": "webrunner",
        "PACK-TOOL-ROOT/resources/inst/inst.msi.py": "inst.py",
        "html5-sandbox-app/testscripts": "html5-sandbox-app/testscripts",
        "tests.full.xml": "tests.full.xml",
        "tests.xml": "tests.xml"
      },
      "pkg-app": {
        "apk-common-opts": "--enable-remote-debugging",
        "copylist": {
          "PACK-TOOL-ROOT/resources/testharness": "resources",
          "PACK-TOOL-ROOT/resources/webrunner": "webrunner",
          "icon.png": "icon.ico"
        }
      },
      "subapp-list": {
        "html5-sandbox-app": {
          "apk-common-opts": "--enable-remote-debugging",
          "apk-type": "MANIFEST",
          "copylist": {
            "../sandbox/w3c/bdd": "w3c",
            "../sandbox/w3c/support": "w3c/support"
          }
        }
      }
    }
  },
  "pkg-name": "tct-sandbox-html5-tests"
}
