{
  "pkg-blacklist": [
    "config.xml",
    "config.xml.*",
    "pack.py",
    "testcase.xsl",
    "testresult.xsl",
    "tests.css",
    "icon.png",
    "manifest.json",
    "suite.json",
    "*-py",
    "inst.*"
  ],
  "pkg-list": {
    "apk,cordova": {
      "blacklist": [],
