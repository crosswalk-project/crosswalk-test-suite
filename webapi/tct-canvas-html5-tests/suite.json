        "tests.full.xml": "tests.full.xml",
        "tests.xml": "tests.xml"
      },
      "pkg-app": {
        "apk-common-opts": "--enable-remote-debugging",
        "blacklist": [],
        "hosted-app": "true"
      },
      "subapp-list": {
        "html5-canvas-app": {
          "apk-common-opts": "--enable-remote-debugging",
          "apk-icon-opt": "",
          "copylist": {
            "../canvas/w3c/bdd": "canvas/w3c"
