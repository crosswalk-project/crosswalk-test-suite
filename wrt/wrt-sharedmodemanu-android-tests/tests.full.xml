<test_definition>
  <suite category="Crosswalk_Runtime_Library" name="wrt-sharedmodemanu-android-tests">
    <set name="sharedmodemanu" type="js">
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_Library_Icon_Check" priority="P1" purpose="Validate if Crosswalk icon display normally" status="approved" type="Functional">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed
          </pre_condition>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_Library_Icon_Check.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_Library_Name_Check" priority="P1" purpose="Validate the installer name is about with Crosswalk" status="approved" type="Functional">
        <description>
          <pre_condition>
            1.Make sure the Crosswalk runtime 'XwalkRuntimeLib.apk'  already exists in the Android OS
          </pre_condition>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_Library_Name_Check.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_mismatch_x86_embeded" priority="P2" purpose="Validate descriptive error message prompt if install arm app in x86 platform" status="approved" type="Functional">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed
            2.Make sure test device is arm architecture
          </pre_condition>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_mismatch_x86_embeded.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_mismatch_arm_embeded" priority="P2" purpose="Validate descriptive error message prompt if install x86 app in arm platform" status="approved" type="Functional">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed
            2.Make sure test device is x86 architecture
          </pre_condition>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_mismatch_arm_embeded.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_mismatch_arm_shared" priority="P2" purpose="Validate descriptive error message prompt if install shared arm app in arm platform" status="approved" type="Functional">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_mismatch_arm_shared.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_mismatch_arm_shared_x86_crosswalk" priority="P2" purpose="Validate descriptive error message prompt if install shared arm app in arm platform with x86 RuntimeLib" status="approved" type="Functional">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_mismatch_arm_shared_x86_crosswalk.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_mismatch_x86_shared" priority="P2" purpose="Validate descriptive error message prompt if install shared x86 app in x86 platform" status="approved" type="Functional">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_mismatch_x86_shared.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_mismatch_x86_shared_arm_crosswalk" priority="P2" purpose="Validate descriptive error message prompt if install shared x86 app in x86 platform with arm RuntimeLib" status="approved" type="Functional">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_mismatch_x86_shared_arm_crosswalk.html</test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>