      <testcase purpose="Validate Crosswalk support Chinese language successfully" type="Functional" status="approved" component="Crosswalk Runtime Binary" execution_type="manual" priority="P1" id="Crosswalk_Launcher_List_Apps">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-commandline-tizen-tests/commandline/Crosswalk_Launcher_List_Apps.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate a feature is enabled  in Crosswalk" type="Functional" status="designed" component="Crosswalk Runtime Binary" execution_type="manual" priority="P1" id="Crosswalk_Launcher_Flag_Enable">
