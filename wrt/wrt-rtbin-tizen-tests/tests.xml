<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="Crosswalk Runtime Binary" launcher="xwalk" name="wrt-rtbin-tizen-tests">
    <set name="rtbin">
      <testcase component="Crosswalk Runtime Binary" execution_type="manual" id="Crosswalk_Launcher_Check_Project_Name" purpose="Validate the project name is about with Crosswalk">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is download to local.
          </pre_condition>
          <test_script_entry>/opt/wrt-rtbin-tizen-tests/rtbin/Crosswalk_Launcher_Check_Project_Name.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk Runtime Binary" execution_type="manual" id="Crosswalk_Launcher_i18n_Chinese" purpose="Validate Crosswalk support Chinese language successfully">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-rtbin-tizen-tests/rtbin/Crosswalk_Launcher_i18n_Chinese.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk Runtime Binary" execution_type="manual" id="Crosswalk_Launcher_i18n_English" purpose="Validate Crosswalk support English language successfully">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-rtbin-tizen-tests/rtbin/Crosswalk_Launcher_i18n_English.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk Runtime Binary" execution_type="manual" id="Crosswalk_Launcher_List_Apps" purpose="Validate Crosswalk support Chinese language successfully">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-rtbin-tizen-tests/rtbin/Crosswalk_Launcher_List_Apps.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk Runtime Binary" execution_type="manual" id="Crosswalk_Feature_Flags_disable-device-capabilities" purpose="Validate Crosswalk support '--disable-device-capabilities' flag successfully">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-rtbin-tizen-tests/rtbin/Crosswalk_Feature_Flags_disable-device-capabilities.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk Runtime Binary" execution_type="manual" id="Crosswalk_Feature_Flags_fullscreen" purpose="Validate Crosswalk support '--fullscreen' flag successfully">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-rtbin-tizen-tests/rtbin/Crosswalk_Feature_Flags_fullscreen.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk Runtime Binary" execution_type="manual" id="Crosswalk_Feature_Flags_list-feature-flags" purpose="Validate Crosswalk support '--list-feature-flags' flag successfully">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-rtbin-tizen-tests/rtbin/Crosswalk_Feature_Flags_list-feature-flags.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk Runtime Binary" execution_type="manual" id="Crosswalk_Run_As_Service_Launch" purpose="Validate Crosswalk can launch apps in the 'run-as-service' mode">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed
            2.Make sure the "SysApps_DeviceCapabilities.xpk" package is installed
          </pre_condition>
          <test_script_entry>/opt/wrt-rtbin-tizen-tests/rtbin/Crosswalk_Run_As_Service_Launch.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk Runtime Binary" execution_type="manual" id="Crosswalk_Binary_Install" purpose="Validate Crosswalk install is successfull">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is push device.
          </pre_condition>
          <test_script_entry>/opt/wrt-rtbin-tizen-tests/rtbin/Crosswalk_Binary_Install.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk Runtime Binary" execution_type="manual" id="Crosswalk_Binary_Uninstall" purpose="Validate Crosswalk unistall successfull">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed
          </pre_condition>
          <test_script_entry>/opt/wrt-rtbin-tizen-tests/rtbin/Crosswalk_Binary_Uninstall.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk Runtime Binary" execution_type="manual" id="Crosswalk_Feature_Flags_capabilitiesAndFullscreen" purpose="Validate use combination flags capabilities and fullscreenthe result is normally">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed
          </pre_condition>
          <test_script_entry>/opt/wrt-rtbin-tizen-tests/rtbin/Crosswalk_Feature_Flags_capabilitiesAndFullscreen.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk Runtime Binary" execution_type="manual" id="Crosswalk_Feature_Flags_negativeFlagsCapabilities" purpose="Validate use negative flags the capabilities result normally">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed
          </pre_condition>
          <test_script_entry>/opt/wrt-rtbin-tizen-tests/rtbin/Crosswalk_Feature_Flags_negativeFlagsCapabilities.html</test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>
